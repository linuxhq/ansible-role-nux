[nux-dextop]
name=Nux.Ro RPMs for general desktop use
baseurl=http://li.nux.ro/download/nux/dextop/{{ nux_dist }}/$basearch/ http://mirror.li.nux.ro/li.nux.ro/nux/dextop/{{ nux_dist }}/$basearch/
enabled={{ '1' if nux_repository_nux_desktop|bool else '0' }}
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-nux.ro
protect=0

[nux-dextop-testing]
name=Nux.Ro RPMs for general desktop use - testing
baseurl=http://li.nux.ro/download/nux/dextop-testing/{{ nux_dist }}/$basearch/
enabled={{ '1' if nux_repository_nux_desktop_testing|bool else '0' }}
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-nux.ro
protect=0
